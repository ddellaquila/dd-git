[user]
	name = Write Your Name Here
	email = youraddress@example.com

[github]
	user = your-github-account
	token = your-github-token

[core]
	autocrlf = false
	safecrlf = true
	editor = vim
	excludesfile = /home/YOUR-HOME/.gitignore-global

[alias]
	st = status
	co = checkout
	ci = commit
	ca = commit --reset-author --amend
	unmodify = checkout --
	unstage = reset HEAD --
	br = branch
	last = log -1 --format=fuller HEAD
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
	type = cat-file -t
	dump = cat-file -p
	tag-list = tag -l -n1
	show-names = show --name-only

[merge]
	tool = gvimdiff

[init]
	templatedir = ~/.git_templates

[color]
	diff = auto
	status = auto
	branch = auto
